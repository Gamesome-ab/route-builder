{
	"name": "root",
	"version": "0.0.0",
	"private": true,
	"devDependencies": {
		"@eslint/js": "9.39.2",
		"@nx/esbuild": "22.3.3",
		"@nx/eslint": "22.3.3",
		"@nx/eslint-plugin": "22.3.3",
		"@nx/js": "22.3.3",
		"@nx/vitest": "22.3.3",
		"@swc-node/register": "1.11.1",
		"@swc/core": "1.15.7",
		"@swc/helpers": "0.5.18",
		"@types/node": "25.0.3",
		"@vitest/coverage-v8": "4.0.16",
		"esbuild": "0.27.2",
		"eslint": "9.39.2",
		"eslint-config-prettier": "10.1.8",
		"jsonc-eslint-parser": "2.4.2",
		"nx": "22.3.3",
		"prettier": "3.7.4",
		"tslib": "2.8.1",
		"typescript": "5.9.3",
		"typescript-eslint": "8.50.1",
		"verdaccio": "6.2.4",
		"vite": "7.3.0",
		"vitest": "4.0.16"
	},
	"dependencies": {},
	"scripts": {
		"prepare": "git config core.hooksPath .githooks",
		"tltb": "nx run-many --target=typecheck,lint,test,build --all && nx format:check --verbose --all",
		"pre-push": "pnpm tltb",
		"pre-commit": "pnpm tltb",
		"prepack": "pnpm tltb",
		"prepublishOnly": "pnpm tltb"
	},
	"nx": {
		"includedScripts": [],
		"targets": {
			"local-registry": {
				"executor": "@nx/js:verdaccio",
				"options": {
					"port": 4873,
					"config": ".verdaccio/config.yml",
					"storage": "tmp/local-registry/storage"
				}
			}
		}
	}
}
